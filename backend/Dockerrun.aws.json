{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "application-api",
      "image": "ninjacyborg/application-api:1.0",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8081
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://localhost:5432/patron"
        }
      ]
    }
  ]
}